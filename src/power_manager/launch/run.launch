<launch>
    <arg name="robot" value="02"/>
    <machine name="neato" address="192.168.150.1$(arg robot)" user="odroid" default="false" env-loader="/home/odroid/catkin_ws/devel/env.sh"/>
	<machine name="artlab" address="192.168.150.1" user="$(env USER)" default="true" env-loader="$(env HOME)/catkin_ws/devel/env.sh"/>
	   
	<rosparam command="load" file="$(env HOME)/catkin_ws/params/myparams.yaml"/>

	<node name="power_manager" pkg="power_manager" type="__init__.py" output="screen" machine="neato" args="-sever-debug" >
	
	</node>
</launch>
