    Help Height: 0
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /TF1/Frames1
        - /Odometry1
        - /Map1
        - /ParticleCloud1
        - /Path1
        - /Scenario.Path1
      Splitter Ratio: 0.363049
    Tree Height: 437
  - Class: rviz/Selection
    Name: Selection
  - Class: rviz/Tool Properties
    Expanded:
      - /2D Pose Estimate1
      - /2D Nav Goal1
